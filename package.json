{
  "name": "next-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docker:build": "docker build --build-arg ARGS_NODE_ENV=production -t next-boilerplate .",
    "docker:run": "docker run -p 3000:3000 next-boilerplate",
    "build:production": "next build",
    "build:staging": "next build",
    "build:develop": "npm run storybook:build && npm run storybook:move && next build",
    "storybook": "storybook dev -p 6006",
    "storybook:move": "mv ./storybook-static ./public/storybook",
    "storybook:build": "rm -rf ./public/storybook && storybook build"
  },
  "dependencies": {
    "@iconify-icon/react": "^2.1.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "axios": "^1.7.2",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "next": "^14.2.3",
    "nextjs-toploader": "^1.6.12",
    "postgres": "^3.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-ga4": "^2.1.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@iconify/react": "^5.0.0-beta.5",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.10",
    "@storybook/nextjs": "^7.6.10",
    "@storybook/react": "^7.6.10",
    "@storybook/test": "^7.6.10",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-testing-library": "^6.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "storybook": "^7.6.10",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animated": "^1.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.15.0"
  }
}
